<%@page import="java.util.Locale"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags" %><!DOCTYPE HTML><html><head>    <title>Register</title>    <meta charset="UTF-8" />    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>    <script src="https://cdn.jsdelivr.net/jquery.validation/1.15.0/jquery.validate.min.js"></script>    <script src="https://cdn.jsdelivr.net/jquery.validation/1.15.0/additional-methods.min.js"></script>    <script src='https://www.google.com/recaptcha/api.js'></script>    <script type="text/javascript" src="${pageContext.servletContext.contextPath}/resources/js/form_validate.js"></script>    <script type="text/javascript" src="${pageContext.servletContext.contextPath}/resources/js/password_validate.js"></script>    <script type="text/javascript"     src="${pageContext.servletContext.contextPath}/resources/js/messages_<%= Locale.getDefault().getLanguage() %>.js" ></script><%--     <script type="text/javascript" src="${pageContext.servletContext.contextPath}/resources/js/messages_ru.js" ></script> --%><%--     <script type="text/javascript" src="${pageContext.servletContext.contextPath}/resources/js/messages_en.js" ></script> --%>    <link href="${pageContext.servletContext.contextPath}/resources/css/style.css" rel="stylesheet">    <link href="${pageContext.servletContext.contextPath}/resources/css/inner_style.css" rel="stylesheet"></head><body><h1 align = "center" style="font-size: larger; margin-top: 50px;"><spring:message code="please.register"/></h1><h1 align = "center" style="font-size: larger; margin-top: 10px; color: red;">${message}</h1><spring:message code="login.placeholder" var="login_placeholder"/><spring:message code="password.placeholder" var="password_placeholder"/><spring:message code="password.again.placeholder" var="password_again_placeholder"/><spring:message code="firstName.placeholder" var="firstName_placeholder"/><spring:message code="lastName.placeholder" var="lastName_placeholder"/><spring:message code="email.placeholder" var="email_placeholder"/><spring:message code="birthday.placeholder" var="birthday_placeholder"/><form:form commandName="user" modelAttribute="user" class="box login" style="top:10px;" method="post" id="registerform"           action="${pageContext.request.contextPath}/registered">    <fieldset class="boxBody">        <form:label path="login"><spring:message code="login"/>        <form:input path="login" type="text" tabindex="1"         placeholder="${login_placeholder}"        required="required" name="login"/></form:label>            <form:errors path="login" cssStyle="color:red"/>        <form:label path="password"><spring:message code="password"/>            <script type="text/javascript">                function checkPassword() {                    $.ajax({                        url: 'checkPassword',                        data: ({password: $('#password').val()}),                        success: function (data) {                            $('#strengthValue').html(data);                        }                    });                }            </script>        <form:password path="password" onkeyup="checkPassword()" tabindex="2"         placeholder="${password_placeholder}"  id="password"                       required="required" name="password" maxlength="32"                       /></form:label>        <span style="float: right" id="strengthValue"></span>            <form:errors path="password" cssStyle="color:red"/>        <form:label path="passwordagain"><spring:message code="password.again"/>        <form:password path="passwordagain"  tabindex="2" placeholder="${password_again_placeholder}" id="passwordagain"                       required="required" name="password again" maxlength="32"                       onChange="checkPasswordMatch();"/></form:label>        <span style="float: right"></span>            <form:errors path="passwordagain" cssStyle="color:red"/>        <div class = "registrationFormAlert" id="divCheckPasswordMatch" style="color:red;"></div>        <form:label path="firstName"><spring:message code="name"/>        <form:input path="firstName" type="text" tabindex="2" placeholder="${firstName_placeholder}" required="required" name="name"/></form:label>            <form:errors path="firstName" cssStyle="color:red"/>        <form:label path="lastName"><spring:message code="surname"/>        <form:input path="lastName" type="text" tabindex="2" placeholder="${lastName_placeholder}" required="required" name="lastName"/></form:label>            <form:errors path="lastName" cssStyle="color:red"/>        <form:label path="email"><spring:message code="email"/>        <form:input path="email" type="text" tabindex="2" placeholder="${email_placeholder}" required="required" name="email"/></form:label>            <form:errors path="email" cssStyle="color:red"/>        <form:label path="birthday"><spring:message code="birthday"/>        <form:input path="birthday" type="text" tabindex="2" placeholder="${birthday_placeholder}" required="required" name="birthday"/></form:label>            <form:errors path="birthday" cssStyle="color:red"/>        <form:input path="role" type="hidden" tabindex="1" name="role"/>        <sec:authorize access="hasRole('ANONYMOUS')">            <script src='https://www.google.com/recaptcha/api.js'></script>            <div class="g-recaptcha" data-sitekey="6LdJSQgUAAAAAPjZJyKXGMjBhojxoEIKPHfjBAwi"></div>        </sec:authorize>    </fieldset>    <footer>        <input type="submit" id="submitButton" class="btnLogin" value="<spring:message code="register.button"/>" tabindex="4">    </footer></form:form></body></html>